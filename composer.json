{
	"name": "4training/4training-skin",
	"type": "mediawiki-skin",
	"description": "Modern version of the MonoBook skin",
	"keywords": [
		"wiki",
		"MediaWiki",
		"skin"
	],
	"homepage": "https://github.com/4training/ForTraining",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Trevor Parscal"
		},
		{
			"name": "Roan Kattouw"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": ">1.0.12"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "0.9.*",
		"mediawiki/mediawiki-codesniffer": "0.3.0"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor",
			"phpcs -p"
		]
	}

}
